{
  "include": ["src"],
  "compileOnSave": false,
  "compilerOptions": {
    "noEmit": true,
    "sourceMap": false,
    "module": "commonjs",
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "target": "es5",
    "lib": ["dom", "es6"],
    "allowJs": false,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "baseUrl": "src",
    "jsx": "react"
  }
}
